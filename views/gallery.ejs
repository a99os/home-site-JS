<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs") %>
  <style>
    .container {
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
    }
    .alohida {
      margin: 20px;
      padding: 5px;
      display: inline-block;
      box-shadow: rgba(0, 0, 0, 0.4) 0px 2px 4px,
        rgba(0, 0, 0, 0.3) 0px 7px 13px -3px,
        rgba(0, 0, 0, 0.2) 0px -3px 0px inset;
    }
    img {
      width: 350px;
      height: 300px;
    }
  </style>
  <body>
    <nav><%- include("./partials/nav.ejs") %></nav>
    <h1>Gallary Page</h1>
    <div class="container">
      <% img.forEach(rasm=>{%>
      <div class="alohida"><img src="<%=rasm%>" alt="rasm" /></div>
      <% })%>
    </div>
  </body>
</html>
